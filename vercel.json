{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/frontend"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/index.html"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}